<!-- Use this file to add front end struts actions. You override backend portal struts action by providing an 
Action with the same path attribute or a form-bean with the same name  -->


<form-bean name="logConsoleForm" type="com.dotmarketing.logConsole.form.ConsoleLogForm" ></form-bean> 


<action path="/ext/logConsole/viewConsole" type="com.dotmarketing.logConsole.business.LogConfigAction" name="logConsoleForm">
	<forward name="portlet.ext.plugins.logMan.struts" path="portlet.ext.plugins.logMan.struts" />
</action>

<action path="/ext/logConsole/runConsole" type="com.dotmarketing.logConsole.business.UpdateLogConfigAction" name="logConsoleForm">
	<forward name="portlet.ext.plugins.logMan.struts" path="portlet.ext.plugins.logMan.struts" />
</action>