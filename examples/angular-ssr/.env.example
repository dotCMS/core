# dotCMS Configuration
# URL of your dotCMS instance
DOTCMS_URL=https://demo.dotcms.com

# Authentication token for dotCMS API
# Generate this token from your dotCMS admin panel
DOTCMS_AUTH_TOKEN=your_auth_token_here

# Site ID for your dotCMS site
# You can find this in your dotCMS admin panel or use 'YOUR_SITE_ID' as default
DOTCMS_SITE_ID=YOUR_SITE_ID

# Server Configuration
# Port number for the Express server
PORT=4000