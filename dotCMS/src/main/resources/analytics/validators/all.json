{
  "context": {
    "type": "json_object",
    "required": true,

    "allowed_attributes": {
      "site_auth": {
        "type": "string",
        "required": true,
        "custom-validator": "SiteKeyValidator"
      },
      "session_id": {
        "type": "string",
        "required": true
      },
      "user_id": {
        "type": "string",
        "required": true
      }
    }
  },
  "events": {
    "type": "json_array",
    "array_type": "json_object",
    "required": true,

    "allowed_attributes": {
        "event_type": {
          "type": "string",
          "required": true
        },
        "local_time": {
          "type": "date",
          "required": true
        },
        "data": {
          "type": "json_object",

          "allowed_attributes": {
            "custom": {
              "type": "json_object"
            }
          }
        }
    }
  }
}