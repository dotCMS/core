<div style="text-align: $!item.attrs.textAlign">

    #if ($!item.attrs.href != "null")
    <a href="$!item.attrs.href">
    #end
        <img
            src="#if($!item.attrs.src && $!item.attrs.src != "null")$!item.attrs.src#{else}$!item.attrs.data.asset#end"
            alt="$!item.attrs.alt"
            title="$!item.attrs.title"
        />
    #if ($!item.attrs.href != "null")
    </a>
    #end

</div>

