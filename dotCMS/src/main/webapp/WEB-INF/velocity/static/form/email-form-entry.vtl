	</tr>
	 <tr>
		 <td valign="top">
			<b>$content.modDate</b>
		</td>
		<td valign="top">
			<b>$content.modUser</b>
		</td>
	</tr>
   #for($field in $contentType.fields())
      <tr bgcolor="white">
          <td align="right">
            <font color="gray"><b>$field.name()</b></font>
         </td>
         <td>
            $content.get($field.variable())
         </td>
      </tr>
   #end

</table>


