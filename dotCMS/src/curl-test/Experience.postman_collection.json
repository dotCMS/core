							"",
							"var jsonData = pm.response.json();",
							"",
							"pm.test(\"All Metrics included\", function () {",
							"    var metrics = jsonData;",
							"    pm.expect(metrics.ACTIVE_USERS).to.be.not.undefined;",
							"    pm.expect(metrics.CONTENTS_RECENTLY_EDITED).to.be.not.undefined;",
							"    pm.expect(metrics.CONTENTS_TOTAL).to.be.not.undefined;",
							"    pm.expect(metrics.CONTENT_LAST_EDIT).to.be.not.undefined;",
							"    pm.expect(metrics.CUSTOM_CONTENT_TYPES).to.be.not.undefined;",
							"    pm.expect(metrics.LAST_LOGIN).to.be.not.undefined;",
							"    pm.expect(metrics.LAST_LOGIN_USER).to.be.not.undefined;",
							"    pm.expect(metrics.SITES_ACTIVE_SITES).to.be.not.undefined;",
							"    pm.expect(metrics.SITES_NON_SYSTEM_FIELDS).to.be.not.undefined;",
							"    pm.expect(metrics.WORKFLOW_ACTIONS).to.be.not.undefined;",
							"    pm.expect(metrics.WORKFLOW_CONTENT_TYPES_ASSIGNED_SCHEMES).to.be.not.undefined;",
							"    pm.expect(metrics.WORKFLOW_SCHEMES).to.be.not.undefined;",
							"    pm.expect(metrics.WORKFLOW_SCHEMES_ARCHIVED_INCLUDED).to.be.not.undefined;",
							"    pm.expect(metrics.WORKFLOW_STEPS).to.be.not.undefined;",
