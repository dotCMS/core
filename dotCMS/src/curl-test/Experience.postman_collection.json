		{
			"name": "gettin Plusings",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code should be ok 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"",
							"pm.test(\"It plusing there\", function () {",
							"    var plugins = jsonData;",
							"    var pluginIntalled = plugins",
							"    .filter(plugin => plugin.symbolicName === \"com.dotcms.experience\");",
							"",
							"console.log(pluginIntalled);",
							"    pm.expect(1).to.be.equal(pluginIntalled.length);",
							"    pm.expect(\"com.dotcms.experience-1.0.jar\").to.be.equal(pluginIntalled[0].jarFile);",
							"    pm.expect(32).to.be.equal(pluginIntalled[0].state);",
							"",
							"",
							"});",
							"",
							"",
							"",
							"",
							"",
							"",
							""
						],
