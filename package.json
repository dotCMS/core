{
    "name": "dotcms-ui",
    "version": "5.2.9",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test": "ng test",
        "createpackagejson": "node -e \"var data = require('./dist/package.json');var fs = require('fs');data.version = data.version + \"-\" + new Date().getTime();var {scripts, dependencies, devDependencies, ...res} = data;fs.writeFileSync('./dist/package.json', JSON.stringify(res, null, 4));\"",
        "publish:dev": "rm -rf dist;npm run build:libs;npm run build:prod;cp package.json ./dist/package.json;npm run createpackagejson;cd dist;npm publish",
        "publish:doc:docms": "typedoc projects/dotcms/src/lib/ --options projects/dotcms/typedoc.json; gh-pages --dist ./dist-docs/dotcms --dest ./docs/dotcms",
        "publish:doc:docms-models": "typedoc projects/dotcms-models/src/ --options projects/dotcms-models/typedoc.json; gh-pages --dist ./dist-docs/dotcms-models --dest ./docs/dotcms-models",
        "publish:storybook": "build-storybook --config-dir .storybook --output-dir .out/design;storybook-to-ghpages --existing-output-dir=.out; rm -rf .out",
        "publish:docs": "npm run publish:storybook;npm run publish:doc:docms;npm run publish:doc:docms-models",
        "build:dev": "ng build --configuration=development",
        "build:prod": "ng build --configuration=production --prod dotcms-ui",
        "build:libs": "ng build dotcms-js; ng build dot-rules; ng build dot-layout-grid",
        "docker:test": "npm run docker:build;IMAGE_BASE_NAME=core-web docker-compose -f docker-compose-running-test.yml up",
        "docker:build": "docker build -t core-web .",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dotCMS/core-web.git"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/cdk": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/http": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@ctrl/tinycolor": "^2.0.0",
        "angular2-hotkeys": "2.1.2",
        "angular2-tinymce": "^2.1.2",
        "core-js": "^2.5.4",
        "dotcms-models": "^0.0.8",
        "font-awesome": "^4.7.0",
        "lodash": "^4.17.11",
        "md5": "^2.2.1",
        "moment": "^2.19.4",
        "ng2-ace-editor": "^0.3.8",
        "ng2-dragula": "^2.1.0",
        "ngx-md": "^7.1.3",
        "node-fetch": "^2.6.0",
        "npm": "^6.9.0",
        "primeicons": "^1.0.0",
        "primeng": "^7.0.3",
        "rxjs": "~6.3.3",
        "test": "^0.6.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.11.4",
        "@angular-devkit/build-ng-packagr": "~0.11.4",
        "@angular/cli": "~7.1.4",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@babel/core": "^7.7.2",
        "@storybook/addon-actions": "^5.2.6",
        "@storybook/addon-links": "^5.2.6",
        "@storybook/addon-notes": "^5.2.6",
        "@storybook/addons": "^5.2.6",
        "@storybook/angular": "^5.2.6",
        "@storybook/storybook-deployer": "^2.8.1",
        "@types/googlemaps": "^3.30.1",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "babel-loader": "^8.0.6",
        "codelyzer": "~4.5.0",
        "gh-pages": "^2.0.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.1",
        "karma-html-reporter": "^0.2.7",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "mock-socket": "^8.0.5",
        "ng-packagr": "^4.4.5",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": "~0.31.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "tslint-angular": "^1.1.1",
        "typescript": "~3.1.6"
    }
}
