{
  "name": "dotcms-ui",
  "version": "4.2.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --port 3000",
    "build": "ng build --prod --aot=false --base-href=\"./\"",
    "watch": "npm run build -w --no-delete-output-path --sourcemap",
    "test": "ng test -sr",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "devversionbump": "node -e \"var data = require('./dist/package.json');var fs = require('fs');data.version = data.version + \"-\" + new Date().getTime();fs.writeFileSync('./dist/package.json', JSON.stringify(data, null, 4));\"",
    "publishdev": "rm -rf dist;npm run build;cp package.json ./dist/package.json;npm run devversionbump;cd dist;npm publish",
    "dotcms": "npm run build -- -w --sourcemap=false"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "angular2-grid": "^2.1.0",
    "angular2-hotkeys": "^2.0.3",
    "angular2-logger": "^0.5.1",
    "angular2-tinymce": "^2.1.2",
    "core-js": "^2.5.3",
    "dotcms-rules-engine": "^0.0.6",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.19.4",
    "ng2-ace-editor": "^0.2.5",
    "ng2-dragula": "^1.5.0",
    "primeng": "^4.3.0",
    "rxjs": "^5.5.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@types/googlemaps": "^3.30.1",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.93",
    "codelyzer": "~3.1.1",
    "dotcms-js": "4.2.0-b93",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webpack": "^3.10.0"
  }
}
