{
    "name": "dotcms-ui",
    "version": "21.01.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test": "ng test",
        "dev": "ng serve --hmr --live-reload=false",
        "createpackagejson": "node -e \"var data = require('./dist/package.json');var fs = require('fs');data.version = data.version + \"-\" + new Date().getTime();var {scripts, dependencies, devDependencies, ...res} = data;fs.writeFileSync('./dist/package.json', JSON.stringify(res, null, 4));\"",
        "publish:dev": "rm -rf dist;npm run build:prod;cp package.json ./dist/package.json;npm run createpackagejson;cd dist;npm publish",
        "publish:doc:docms": "typedoc projects/dotcms/src/lib/ --options projects/dotcms/typedoc.json; gh-pages --dist ./dist-docs/dotcms --dest ./docs/dotcms",
        "publish:doc:docms-models": "typedoc projects/dotcms-models/src/ --options projects/dotcms-models/typedoc.json; gh-pages --dist ./dist-docs/dotcms-models --dest ./docs/dotcms-models",
        "publish:docs": "npm run publish:doc:docms && npm run publish:doc:docms-models",
        "build:dev": "ng build --configuration=development",
        "build:prod": "ng build --configuration=production dotcms-ui",
        "build:libs": "ng build dotcms-js; ng build dot-rules; ng build dot-layout-grid",
        "docker:test": "npm run docker:build;IMAGE_BASE_NAME=core-web docker-compose -f docker-compose-running-test.yml up",
        "docker:build": "docker build -t core-web .",
        "analyze": "ng build --stats-json;webpack-bundle-analyzer dist/stats.json",
        "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
        "storybook": "npm run docs:json && start-storybook -p 6006",
        "build-storybook": "npm run docs:json && build-storybook",
        "deploy-docs": "storybook-to-ghpages --ci && npm run publish:docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dotCMS/core-web.git"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "^11.0.3",
        "@angular/cdk": "^11.0.2",
        "@angular/common": "^11.0.3",
        "@angular/compiler": "^11.0.3",
        "@angular/core": "^11.0.3",
        "@angular/forms": "^11.0.3",
        "@angular/platform-browser": "^11.0.3",
        "@angular/platform-browser-dynamic": "^11.0.3",
        "@angular/router": "^11.0.3",
        "@ctrl/tinycolor": "^3.1.7",
        "@ngrx/component-store": "^10.0.1",
        "dotcms-webcomponents": "0.0.18",
        "font-awesome": "^4.7.0",
        "lodash": "^4.17.20",
        "md5": "^2.3.0",
        "moment": "^2.29.1",
        "ng2-dragula": "^2.1.1",
        "ngx-markdown": "^10.1.1",
        "ngx-monaco-editor": "^9.0.0",
        "node-fetch": "^2.6.1",
        "npm": "^6.14.9",
        "primeflex": "^2.0.0",
        "primeicons": "^4.1.0",
        "primeng": "^11.0.0",
        "rxjs": "~6.6.3",
        "test": "^0.6.0",
        "tslib": "^2.0.3",
        "zone.js": "^0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1100.3",
        "@angular-devkit/build-ng-packagr": "~0.1002.0",
        "@angular/cli": "^11.0.3",
        "@angular/compiler-cli": "^11.0.3",
        "@angular/language-service": "^11.0.3",
        "@babel/core": "^7.12.9",
        "@compodoc/compodoc": "^1.1.11",
        "@storybook/addon-actions": "^6.1.10",
        "@storybook/addon-essentials": "^6.1.10",
        "@storybook/addon-links": "^6.1.10",
        "@storybook/addons": "^6.1.10",
        "@storybook/angular": "^6.1.10",
        "@storybook/storybook-deployer": "^2.8.7",
        "@storybook/theming": "^6.1.10",
        "@types/googlemaps": "3.40.3",
        "@types/jasmine": "~3.6.2",
        "@types/jasminewd2": "~2.0.8",
        "babel-loader": "^8.2.1",
        "codelyzer": "^6.0.1",
        "gh-pages": "^3.1.0",
        "husky": "^4.3.5",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "~5.2.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-html-reporter": "^0.2.7",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "mock-socket": "^9.0.3",
        "ng-packagr": "^11.0.3",
        "prettier": "^2.2.0",
        "pretty-quick": "^3.1.0",
        "protractor": "~7.0.0",
        "ts-node": "~9.0.0",
        "tslint": "~6.1.3",
        "tslint-angular": "^3.0.3",
        "typescript": "~4.0.3",
        "webpack-bundle-analyzer": "^4.1.0"
    }
}
