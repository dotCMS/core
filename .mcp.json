{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"],
      "env": {}
    },
    "primeng": {
      "command": "npx",
      "args": ["-y", "@primeng/mcp"]
    },
    "angular-cli": {
      "type": "stdio",
      "command": "npx",
      "args": ["@angular/cli", "mcp"],
      "env": {}
    }
  }
}